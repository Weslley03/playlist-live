<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>playlist music</title>
</head>
<body>
    <h1>Olá EJS</h1>
    <a href="/admin">adm</a>

        <% playlist.forEach((music) => { %>
    
        <section>
            <h2> <%= music.name %> </h2>
            <p> <%= music.author %> </p>
            <img src="<%= music.linkImage %>" alt="logo da música">
            <audio controls src="<%= music.linkMusic %>"></audio>
        </section>

        <% }) %>
</body>
</html>